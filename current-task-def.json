

    "containerDefinitions": [
        {
            "name": "prodready-infra-backend",
            "image": "875486186130.dkr.ecr.us-east-1.amazonaws.com/prodready-infra-api:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "DYNAMODB_TABLE_NAME",
                    "value": "prodready-infra-items-staging"
                },
                {
                    "name": "NODE_ENV",
                    "value": "staging"
                },
                {
                    "name": "DATABASE_URL",
                    "value": "postgresql://appadmin:TempPassword123!@prodready-infra-db-staging.c4nkc6wistr3.us-east-1.rds.amazonaws.com:5432/prodready_infra_staging"
                },
                {
                    "name": "PORT",
                    "value": "8080"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/prodready-infra-backend-staging",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8080/api/health || exit 1"
                ],
                "interval": 30,
                "timeout": 10,
                "retries": 3,
                "startPeriod": 60
            },
            "systemControls": []
        }
    ],
    "family": "prodready-infra-backend-staging",
    "taskRoleArn": "arn:aws:iam::875486186130:role/prodready-infra-task-role-staging",
    "executionRoleArn": "arn:aws:iam::875486186130:role/prodready-infra-task-execution-role-staging",
    "networkMode": "awsvpc",
    "volumes": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
